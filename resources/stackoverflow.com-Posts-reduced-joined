&#xA; StreamReader errorReader = p.StandardError;&#xA;&#xA; // Assign a Stream Writer to send the Commands&#xA; StreamWriter commandWriter = p.StandardInput;&#xA;&#xA; // Send the &quot;Dir&quot; command to the command line instance.&#xA; commandWriter.Write(&quot;dir&quot;);&#xA; commandWriter.Write(commandWriter.NewLine);&#xA;&#xA; p.WaitForExit();&#xA;&#xA; //Display the result&#xA; Console.WriteLine(outputReader.ReadToEnd());&#xA;&#xA; outputReader.Close();&#xA; errorReader.Close();&#xA; commandWriter.Close();&#xA; }&#xA;&lt;/code&gt;&lt;/pre&gt;&#xA;" OwnerUserId="114298" LastEditorUserId="50776" LastEditDate="2011-01-14T17:00:09.527" LastActivityDate="2011-01-14T17:09:40.100" Title="Can't read output from console command runned from code" Tags="&lt;c#&gt;" AnswerCount="1" FavoriteCount="1" />
